struct Image {
  mut x : Int
  mut y : Int
  dx : Int
  dy : Int
  image : @wasm4.Sprite
}

pub fn draw(self : Image) -> Unit {
  self.image.blit(
    self.x,
    self.y,
    self.dx,
    self.dy,
    { one_bit_per_pixel: false, flip_x: false, flip_y: false, rotate: false },
  )
}

pub fn drawout(self : Image,light:Int) -> Unit {
  outline(self.x-1,self.y-1,self.dx+2,self.dy+2,light,2)
}

struct Coin {
  x : Array[Int]
  y : Array[Int]
  mut num: Int
  mut wf:Int
  dx : Int
  dy : Int
  image : @wasm4.Sprite
}


let enemy_line:Int=51
let me_line:Int=84

pub fn draw(self : Coin) -> Unit {
  @wasm4.set_draw_colors(0,index=4)
  let mut i = self.num
  while i>0{
    i-=1
    if self.wf!=2 && self.wf!=-2{
      if self.x[i]<80 && game.tick%8==0{
        self.x[i]+=1
      }
      if self.x[i]>80 && game.tick%8==0{
        self.x[i]-=1
      }
    }
    self.image.blit(
    self.x[i],
    self.y[i],
    self.dx,
    self.dy,
    { one_bit_per_pixel: false, flip_x: false, flip_y: false, rotate: false },
  )
  }
  if self.wf==0{
    let mut i = self.num
    while i>0{
      i-=1
      if self.y[i]>60{
        self.y[i]-=1
      }
      self.x[i]-=2
      if self.x[i]<=-4{
        self.destroy(i)
      }
    }
  }
  else if self.wf==-1{
    let mut i = self.num
    while i>0{
      i-=1
      if game.tick%2==0{self.y[i]+=1}
      if self.y[i]>=me_line{
        self.destroy(i)
      }
    }
  }
  else if self.wf==1{
    let mut i = self.num
    while i>0{
      i-=1
      if game.tick%2==0{self.y[i]-=1}
      if self.y[i]<=enemy_line{
        self.destroy(i)
      }
    }
  }
  @wasm4.set_draw_colors(4,index=4)
}

fn destroy(self:Coin,index:Int) -> Unit{
  let mut zc=index
    while zc<self.num{//去除该硬币坐标，并继续作画
      self.x[zc]=self.x[zc+1]
      self.y[zc]=self.y[zc+1]
      zc+=1
    }
    self.num-=1
}

