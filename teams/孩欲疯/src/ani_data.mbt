struct StoryTexts {
  x : Int
  y : Int
  length : Int
  mut index : Int
  text : String
  mut part : String
}

pub fn draw(self : StoryTexts) -> Unit {
  @wasm4.text(self.part, self.x, self.y)
  if game.tick%6== 0 {
    if self.index<self.length{self.index+=1}
    self.part = "\{self.part}\{self.text[self.index]}"
  }
}

