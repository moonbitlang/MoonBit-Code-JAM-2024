pub fn set_fall(self:Fall,id:Int,x:Double,y:Double,x_speed:Double,y_speed:Double) -> Unit{
  self.id=id
  self.x=x
  self.y=y
  self.x_speed=x_speed
  self.y_speed=y_speed
}

pub fn set_fall2(self:Fall,text:String,x:Double,y:Double,x_speed:Double,y_speed:Double,g:Double) -> Unit{
  self.text=text
  self.x=x
  self.y=y
  self.x_speed=x_speed
  self.y_speed=y_speed
  self.g=g
}
pub fn draw(self:Fall) -> Unit{
  if self.y>160 || self.y<-40{return}
  if self.x>160 || self.x<-40{return}
  self.x+=self.x_speed
  self.y+=self.y_speed
  self.y_speed+=self.g
  if self.text==""{
    if self.y>92 {return}
  @wasm4.set_draw_colors(0,index=2)
  
  let mut x:Image =boost1
  if self.id==0{
    @wasm4.set_draw_colors(2,index=2)
    return
  }
  else if self.id==1{
    x=boost1
    }
  else if self.id==2{
    x=boost2
    }
  else if self.id==3{
    x=boost3
    }
  else if self.id==4{
    x=boost4
    }
  else if self.id==5{
    x=boost5
    }

    x.image.blit(
      (self.x).to_int(),
      (self.y).to_int(),
      x.dx,
      x.dy,
      { one_bit_per_pixel: false, flip_x: false, flip_y: false, rotate: false },
    )
  @wasm4.set_draw_colors(2,index=2)
  }
  else{
    @wasm4.set_draw_colors(3,index=1)
    @wasm4.set_draw_colors(0,index=2)
    @wasm4.text(self.text,(self.x).to_int(),(self.y).to_int())
    @wasm4.set_draw_colors(1,index=1)
    @wasm4.set_draw_colors(2,index=2)

  }
}


pub fn len(text:String)-> Int{
  let mut i:Int = 0
  while text[i] != '@'{
    i=i+1
  }else{return i+1}
}